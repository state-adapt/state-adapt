{
  "name": "state-adapt",
  "version": "0.26.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "serve:ngxs": "ng serve ngxs-demo",
    "serve:docs": "ng serve docs",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "copy-readme": "cp ./README.md ./dist/libs/core && cp ./README.md ./dist/libs/ngxs && cp ./README.md ./dist/libs/ngrx && cp ./README.md ./dist/libs/react",
    "copy-changelog": "cp ./Changelog.md ./dist/libs/core && cp ./Changelog.md ./dist/libs/ngxs && cp ./Changelog.md ./dist/libs/ngrx && cp ./Changelog.md ./dist/libs/react",
    "copy-files": "run-s copy-readme",
    "bump-version:major": "npm version major --force && cd libs/core && npm version major --force && cd ../ngxs && npm version major --force && cd ../ngrx && npm version major --force && cd ../react && npm version major --force",
    "bump-version:minor": "npm version minor --force && cd libs/core && npm version minor --force && cd ../ngxs && npm version minor --force && cd ../ngrx && npm version minor --force && cd ../react && npm version minor --force",
    "bump-version:patch": "npm version patch --force && cd libs/core && npm version patch --force && cd ../ngxs && npm version patch --force && cd ../ngrx && npm version patch --force && cd ../react && npm version patch --force",
    "build:lib-pack": "ng build core --prod && ng build ngxs --prod && ng build ngrx --prod && ng build react --prod && npm run copy-files && cd dist/libs/core && npm pack && cd ../ngxs && npm pack && cd ../ngrx && npm pack && cd ../react && npm pack",
    "commit-git": "git commit -m \"build: adds new release\" && git push origin && git push origin --tags",
    "pre-publish:minor": "run-s bump-version:minor build:lib-pack commit-git",
    "pre-publish:major": "run-s bump-version:major build:lib-pack commit-git",
    "pre-publish:patch": "run-s bump-version:patch build:lib-pack commit-git",
    "publish:docs": "ng build docs --prod && cp ./apps/docs/src/404.html ./dist/apps/docs/ && rm -rf ../state-adapt.github.io/* && cp -R ./dist/apps/docs/* ../state-adapt.github.io && cd ../state-adapt.github.io && git add . * && git commit -m 'Update' && git push origin master"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@carbon/icons-angular": "^11.2.1",
    "@carbon/themes": "^10.30.0",
    "@ngrx/entity": "^11.0.1",
    "@ngrx/store": "^11.0.1",
    "@ngrx/store-devtools": "^11.0.1",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "@nrwl/angular": "11.5.1",
    "carbon-components": "^10.31.0",
    "carbon-components-angular": "^4.46.0",
    "core-js": "^3.6.5",
    "document-register-element": "1.13.1",
    "lodash": "^4.17.21",
    "ngx-markdown": "^11.1.2",
    "prismjs": "^1.23.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.3",
    "react-router-dom": "5.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "0.13.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "~1.0.0",
    "@angular-eslint/eslint-plugin-template": "~1.0.0",
    "@angular-eslint/template-parser": "~1.0.0",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@nrwl/cli": "11.5.1",
    "@nrwl/cypress": "11.5.1",
    "@nrwl/eslint-plugin-nx": "11.5.1",
    "@nrwl/jest": "11.5.1",
    "@nrwl/linter": "11.5.1",
    "@nrwl/react": "11.6.2",
    "@nrwl/schematics": "^8.12.11",
    "@nrwl/tao": "11.5.1",
    "@nrwl/web": "11.6.2",
    "@nrwl/workspace": "11.5.1",
    "@testing-library/react": "11.1.2",
    "@types/jest": "26.0.8",
    "@types/lodash": "^4.14.168",
    "@types/node": "12.12.38",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-jest": "26.2.2",
    "cypress": "^6.8.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "ng-packagr": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.3"
  }
}
